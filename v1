#include<stdio.h>
#include<string.h>
int  main()
{
	char strExp[]="1+2+2+1+2+5+4-1-3+4-8";
	int a=strExp[0]-'0';
	int i,s=0;
	for(i=0;i<strlen(strExp);i++)
	{
		if(strExp[i]=='+')
		{
			s+=(strExp[i+1]-'0');i++;
		}
	    else if(strExp[i]=='-')
		{
			s-=(strExp[i+1]-'0');i++;
		}
	}
	printf("s=%d\n",s);
	return 0;

}
